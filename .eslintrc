{
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "es6": true,
    "jest": true
  },
  "settings": {
    "import/resolver": {
      "babel-module": {}
    }
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["plugin:jest/recommended", "prettier"],
  "rules": {
    "prettier/prettier": "error",
    "import/prefer-default-export": 0
  }
}
